olan; əksinqilabçı.									<img src="/images/grad.png" align="absmiddle" height="20" style="position:relative; left:-40px; margin-right:-40px" class="os"></a>					<a href="/az/dict/exp/word/12479-ƏKSİNQİLABİ" style="font-size:8pt">					ətraflı					</a>							</div>										<div style="float:right">					<a rel="next" class="catlink" href="/az/dict/exp/byletter/Ə/?p=14">						sonraki					</a> →				</div>										<div style="float:left">← 					<a rel="prev" class="catlink" href="/az/dict/exp/byletter/Ə/?p=12">						əvvəlki					</a>				</div>					</div>	</div>	<div class="col c4">		<div class="wp1">			<div class="flowblock">			</div>		</div>	</div></div>  </div>	<div class="container">		<div class="content">			<div class="footer wp2">				<span class="small">© 2012–2018 Obastan<br><a href="mailto:support@obastan.com">support@obastan.com</span>			</div>		</div>	</div>	<div id="fb-root"></div></body><script>  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');  ga('create', 'UA-1497525-10', 'obastan.com');  ga('require', 'displayfeatures');  var dimensionValue = 'az';  ga('set', 'dimension1', dimensionValue);  if (window.dictionary) ga('set', 'dimension2', window.dictionary);  ga('send', 'pageview');(function (d, w, c) {    (w[c] = w[c] || []).push(function() {        try {            w.yaCounter15700105 = new Ya.Metrika({id:15700105, enableAll: true, webvisor:true});        } catch(e) {}    });    var n = d.getElementsByTagName("script")[0],        s = d.createElement("script"),        f = function () { n.parentNode.insertBefore(s, n); };    s.type = "text/javascript";    s.async = true;    s.src = (d.location.protocol == "https:" ? "https:" : "http:") + "//mc.yandex.ru/metrika/watch.js";    if (w.opera 